RewriteRule ^([^/]+)/([^/]+)/([^/]+).(pdf|doc|docx)$ http://help.it.ox.ac.uk/sites/ithelp/files/resources/$1_$2_$3.$4 [R=302,L]
RewriteRule ^([^/]+)/([^/]+)/([^/]+)/([^/]+).(pdf|doc|docx)$ http://help.it.ox.ac.uk/sites/ithelp/files/resources/$1_$2_$3_$4.$5 [R=302,L]
